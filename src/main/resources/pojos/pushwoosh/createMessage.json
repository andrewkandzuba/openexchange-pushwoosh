{
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "PushWoosh message creation request",
  "type": "object",
  "id": "createMessage",
  "additionalProperties": false,
  "generate-builders": true,
  "properties": {
    "auth": {
      "type": "string",
      "required": true
    },
    "application": {
      "type": "string",
      "required": true
    },
    "notifications": {
      "type": "array",
      "items": {
        "$ref": "notification.json"
      }
    }
  }
}